{
  "name": "kākā",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npx babel src --out-dir dist --extensions .'ts'",
    "test.integration": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r node_modules/ts-node/register -r tsconfig-paths/register  \"test/integration/**/*examples.{j,t}s\" \"test/integration/**/can-*.{j,t}s\" --timeout 10000"
  },
  "author": "",
  "license": "ISC",
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      [
        "@babel/preset-env",
        {
          "modules": "commonjs"
        }
      ]
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ],
          "alias": {
            "core": "./src/core",
            "ports": "./src/ports",
            "adapters": "./src/adapters"
          }
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.3.0",
    "@types/date-fns": "^2.6.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.21",
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "^4.3.6",
    "mocha": "^9.2.2",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "chalk": "^4.1.0",
    "date-fns": "^2.28.0"
  }
}
